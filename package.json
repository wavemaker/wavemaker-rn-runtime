{
  "name": "@wavemaker/app-rn-runtime",
  "version": "0.1.0",
  "description": "''",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "exports": {
    "./": "./lib/commonjs/"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-awesome-module.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test:theme": "jest \"test/styles/theme.spec.ts\"",
    "typescript": "tsc --noEmit",
    "copy-resources": "mkdir -p ./dist && cp -r ./lib/module ./dist && cp ./package.json ./dist/module",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build && node ./scripts/build.js post-build",
    "build:test": "cd ./test && tsc",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "watch": "npm-watch build",
    "test:coverage": "jest --coverage --forceExit; node scripts/combineCoverageHtmlCSS.js"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "http://wavemaker.com",
  "author": "WaveMaker <no-reply@wavemaker.com> (http://wavemaker.com)",
  "license": "MIT",
  "bugs": {
    "url": "http://wavemaker.com/issues"
  },
  "homepage": "http://wavemaker.com#readme",
  "dependencies": {
    "@expo/vector-icons": "15.0.2",
    "@lottiefiles/dotlottie-react": "0.13.5",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native/assets-registry": "0.76.3",
    "@react-navigation/drawer": "7.5.8",
    "@react-navigation/native": "7.1.18",
    "@react-navigation/stack": "7.4.8",
    "@wavemaker/variables": "file:.yalc/@wavemaker/variables",
    "axios": "1.8.3",
    "color": "4.2.3",
    "cross-env": "7.0.3",
    "css-mediaquery": "0.1.2",
    "d3-shape": "3.2.0",
    "expo-application": "7.0.7",
    "expo-font": "14.0.8",
    "expo-linear-gradient": "15.0.7",
    "expo-modules-core": "3.0.20",
    "expo-video": "3.0.11",
    "fast-xml-parser": "4.5.3",
    "imask": "7.6.1",
    "lodash-es": "4.17.21",
    "lottie-react-native": "7.3.4",
    "moment": "2.30.1",
    "react-lottie-player": "2.1.0",
    "react-native-animatable": "1.4.0",
    "react-native-circular-progress": "1.4.1",
    "react-native-logs": "5.1.0",
    "react-native-mime-types": "2.5.0",
    "react-native-paper": "5.12.3",
    "react-native-paper-dates": "0.22.7",
    "react-native-reanimated": "4.1.1",
    "react-native-screens": "4.16.0",
    "react-native-size-scaling": "0.5.1",
    "react-native-svg": "15.12.1",
    "react-native-web": "0.21.0",
    "react-native-webview": "13.15.0",
    "react-xml-parser": "1.1.8",
    "expo-image": "3.0.8",
    "expo-blur": "15.0.7",
    "expo-navigation-bar": "5.0.8",
    "expo-sharing": "14.0.7",
    "expo-file-system": "19.0.14",
    "expo-clipboard": "8.0.7",
    "expo-screen-capture": "8.0.8",
    "expo-secure-store": "15.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-class-static-block": "7.26.0",
    "@babel/preset-env": "7.24.7",
    "@commitlint/config-conventional": "19.2.2",
    "@react-native-community/eslint-config": "3.2.0",
    "@release-it/conventional-changelog": "2.0.0",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.5.0",
    "@types/color": "3.0.6",
    "@types/css-mediaquery": "0.1.4",
    "@types/enzyme": "3.10.18",
    "@types/jest": "29.5.12",
    "@types/lodash-es": "4.17.12",
    "@types/react": "19.1.13",
    "@types/react-native": "0.73.0",
    "@types/react-test-renderer": "18.3.0",
    "babel-jest": "29.7.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "4.0.1",
    "commitlint": "19.3.0",
    "enzyme": "3.11.0",
    "eslint": "9.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-prettier": "5.1.3",
    "execa": "5.0.0",
    "expo-av": "16.0.7",
    "expo-calendar": "15.0.7",
    "expo-camera": "17.0.8",
    "expo-contacts": "15.0.8",
    "expo-device": "8.0.9",
    "expo-document-picker": "14.0.7",
    "expo-file-system": "19.0.14",
    "expo-location": "19.0.7",
    "expo-network": "8.0.7",
    "expo-splash-screen": "31.0.10",
    "expo-web-browser": "15.0.8",
    "handlebars": "4.7.8",
    "jest": "29.7.0",
    "jest-expo": "52.0.2",
    "jest-html-reporter": "3.10.2",
    "metro-react-native-babel-preset": "0.77.0",
    "mockdate": "3.0.5",
    "ncp": "2.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-builder-bob": "0.18.1",
    "react-native-gesture-handler": "2.28.0",
    "react-native-safe-area-context": "5.6.0",
    "react-test-renderer": "19.1.0",
    "release-it": "14.2.2",
    "tar": "6.1.0",
    "typescript": "5.1.3",
    "victory-native": "37.3.2",
    "yargs": "16.2.0"
  },
  "jest": {
    "preset": "jest-expo",
    "testRegex": "(test/.*|(\\.|/)(test|spec))\\.[t]sx?$",
    "setupFiles": [
      "./test/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/.yalc/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/dist/**",
      "!**/scripts/**",
      "!**/typings/**",
      "!**/src/variables/**",
      "!**/src/actions/**",
      "!**/src/runtime/**"
    ],
    "coverageDirectory": "./dist/reports/coverage",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@wavemaker|@wavemaker/app-rn-runtime/components/basic/modal/modal.component|expo-linear-gradient|react-native-gesture-handler|@react-native-community|@react-native-community/datetimepicker|react-native-reanimated|d3*|victory-native|victory-.*)"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash",
      "^@wavemaker/app-rn-runtime/(.*)$": "<rootDir>/src/$1",
      "react-native-webview": "<rootDir>/node_modules/react-native-webview/src/index.ts"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)",
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "reporters": [
      "default",
      [
        "<rootDir>/node_modules/jest-html-reporter",
        {
          "pageTitle": "@wavemaker/app-rn-runtime Test Report",
          "outputPath": "<rootDir>/dist/reports/all-tests.html"
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "ignore": "src/vendor/external.min.js",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": true
    }
  }
}