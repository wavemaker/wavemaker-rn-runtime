{
  "name": "@wavemaker/app-rn-runtime",
  "version": "0.1.0",
  "description": "''",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "type": "module",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-awesome-module.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test:theme": "jest \"test/styles/theme.spec.ts\"",
    "typescript": "tsc --noEmit",
    "copy-resources": "mkdir -p ./dist && cp -r ./lib/module ./dist && cp ./package.json ./dist/module",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build && node ./scripts/build.cjs post-build",
    "build:test": "cd ./test && tsc",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "watch": "npm-watch build",
    "test:coverage": "jest --coverage --forceExit; node scripts/combineCoverageHtmlCSS.js"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "http://wavemaker.com",
  "author": "WaveMaker <no-reply@wavemaker.com> (http://wavemaker.com)",
  "license": "MIT",
  "bugs": {
    "url": "http://wavemaker.com/issues"
  },
  "homepage": "http://wavemaker.com#readme",
  "dependencies": {
    "@expo/vector-icons": "14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native/assets-registry": "0.80.2",
    "@react-navigation/drawer": "6.6.3",
    "@react-navigation/native": "6.1.7",
    "@react-navigation/stack": "6.3.29",
    "@wavemaker/variables": "file:.yalc/@wavemaker/variables",
    "axios": "1.11.0",
    "color": "5.0.0",
    "cross-env": "10.0.0",
    "css-mediaquery": "0.1.2",
    "d3-shape": "3.2.0",
    "expo-application": "6.1.5",
    "expo-blur": "14.1.5",
    "expo-clipboard": "7.1.5",
    "expo-file-system": "18.0.4",
    "expo-font": "13.3.2",
    "expo-image": "2.4.0",
    "expo-linear-gradient": "14.1.5",
    "expo-modules-core": "2.5.0",
    "expo-navigation-bar": "4.2.7",
    "expo-sharing": "13.1.5",
    "expo-video": "2.2.2",
    "fast-xml-parser": "5.2.5",
    "imask": "7.6.1",
    "lodash-es": "4.17.21",
    "lottie-react-native": "7.2.2",
    "moment": "2.30.1",
    "react-lottie-player": "2.1.0",
    "react-native-animatable": "1.4.0",
    "react-native-circular-progress": "1.4.1",
    "react-native-logs": "5.3.0",
    "react-native-mime-types": "2.5.0",
    "react-native-paper": "5.12.3",
    "react-native-paper-dates": "0.22.47",
    "react-native-reanimated": "3.17.4",
    "react-native-screens": "4.11.1",
    "react-native-size-scaling": "0.5.4",
    "react-native-svg": "15.11.2",
    "react-native-web": "0.20.0",
    "react-native-webview": "13.13.5",
    "react-xml-parser": "1.1.8"
  },
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/plugin-transform-class-static-block": "7.27.1",
    "@babel/preset-env": "7.28.0",
    "@commitlint/config-conventional": "19.8.1",
    "@react-native-community/eslint-config": "3.2.0",
    "@release-it/conventional-changelog": "10.0.1",
    "@testing-library/react-native": "13.2.0",
    "@types/color": "4.2.0",
    "@types/css-mediaquery": "0.1.4",
    "@types/enzyme": "3.10.19",
    "@types/jest": "30.0.0",
    "@types/lodash-es": "4.17.12",
    "@types/react": "19.1.8",
    "@types/react-native": "0.73.0",
    "arktype": "2.0.0-dev.12-cjs",
    "babel-jest": "30.0.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "4.0.1",
    "commitlint": "19.8.1",
    "enzyme": "3.11.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-prettier": "5.5.3",
    "execa": "^5.1.1",
    "expo-av": "15.1.7",
    "expo-calendar": "14.1.4",
    "expo-camera": "16.1.11",
    "expo-contacts": "14.2.5",
    "expo-device": "7.1.4",
    "expo-document-picker": "13.1.6",
    "expo-file-system": "18.1.11",
    "expo-location": "18.1.6",
    "expo-network": "7.1.5",
    "expo-splash-screen": "0.30.10",
    "expo-web-browser": "14.2.0",
    "handlebars": "4.7.8",
    "jest": "30.0.5",
    "jest-expo": "53.0.9",
    "jest-html-reporter": "4.3.0",
    "metro-react-native-babel-preset": "0.77.0",
    "mockdate": "3.0.5",
    "ncp": "2.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-builder-bob": "0.40.0",
    "react-native-gesture-handler": "2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "release-it": "19.0.4",
    "tar": "7.4.3",
    "typescript": "5.1.3",
    "victory-native": "37.3.2",
    "yargs": "18.0.0"
  },
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5"
  },
  "jest": {
    "preset": "jest-expo",
    "testRegex": "(test/.*|(\\.|/)(test|spec))\\.[t]sx?$",
    "setupFiles": [
      "./test/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/.yalc/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/dist/**",
      "!**/scripts/**",
      "!**/typings/**",
      "!**/src/variables/**",
      "!**/src/actions/**",
      "!**/src/runtime/**"
    ],
    "coverageDirectory": "./dist/reports/coverage",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@wavemaker|@wavemaker/app-rn-runtime/components/basic/modal/modal.component|expo-linear-gradient|react-native-gesture-handler|@react-native-community|@react-native-community/datetimepicker|react-native-reanimated|d3*|victory-native|victory-.*)"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash",
      "^@wavemaker/app-rn-runtime/(.*)$": "<rootDir>/src/$1",
      "react-native-webview": "<rootDir>/node_modules/react-native-webview/src/index.ts"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)",
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "reporters": [
      "default",
      [
        "<rootDir>/node_modules/jest-html-reporter",
        {
          "pageTitle": "@wavemaker/app-rn-runtime Test Report",
          "outputPath": "<rootDir>/dist/reports/all-tests.html"
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "ignore": "src/vendor/external.min.js",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": true
    }
  }
}
