{
  "name": "@wavemaker/app-rn-runtime",
  "version": "0.1.0",
  "description": "''",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "exports": {
    "./": "./lib/commonjs/"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-awesome-module.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest -u --coverage",
    "typescript": "tsc --noEmit",
    "copy-resources": "mkdir -p ./dist && cp -r ./lib/module ./dist && cp ./package.json ./dist/module",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build && node ./scripts/build.js post-build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "http://wavemaker.com",
  "author": "WaveMaker <no-reply@wavemaker.com> (http://wavemaker.com)",
  "license": "MIT",
  "bugs": {
    "url": "http://wavemaker.com/issues"
  },
  "homepage": "http://wavemaker.com#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.1",
    "@types/lodash-es": "^4.17.4",
    "axios": "^0.21.1",
    "cross-env": "^5.2.0",
    "expression-eval": "^4.0.0",
    "fast-xml-parser": "^3.17.6",
    "lodash-es": "^4.17.21",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-paper": "^4.8.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.2",
    "react-native-unimodules": "~0.12.0",
    "react-native-web": "~0.13.12",
    "react-native-webview": "^11.4.3",
    "react-xml-parser": "^1.1.8",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "commitlint": "^11.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "execa": "^5.0.0",
    "handlebars": "^4.7.7",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-html-reporter": "^3.3.0",
    "ncp": "^2.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-builder-bob": "^0.18.1",
    "react-test-renderer": "^17.0.2",
    "release-it": "^14.2.2",
    "tar": "^6.1.0",
    "typescript": "^4.1.3",
    "yargs": "^16.2.0"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "(test/.*|(\\.|/)(test|spec))\\.[t]sx?$",
    "setupFiles": [
      "./test/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "coverageDirectory": "./dist/reports/coverage",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^lodash-es$": "lodash",
      "^@wavemaker/rn-runtime/(.*)$": "<rootDir>/src/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)",
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "reporters": [
      "default",
      [
        "<rootDir>/node_modules/jest-html-reporter",
        {
          "pageTitle": "@wavemaker/rn-runtime Test Report",
          "outputPath": "<rootDir>/dist/reports/all-tests.html"
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
