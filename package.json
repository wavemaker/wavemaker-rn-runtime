{
  "name": "@wavemaker/app-rn-runtime",
  "version": "0.1.0",
  "description": "''",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "exports": {
    "./": "./lib/commonjs/"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-awesome-module.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest -u --coverage",
    "test:theme": "jest \"test/styles/theme.spec.ts\"",
    "typescript": "tsc --noEmit",
    "copy-resources": "mkdir -p ./dist && cp -r ./lib/module ./dist && cp ./package.json ./dist/module",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build && node ./scripts/build.js post-build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "watch": "npm-watch build",
    "publish-patch": "npm run build -- --production=true && node scripts/npm-publish.js"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "http://wavemaker.com",
  "author": "WaveMaker <no-reply@wavemaker.com> (http://wavemaker.com)",
  "license": "MIT",
  "bugs": {
    "url": "http://wavemaker.com/issues"
  },
  "homepage": "http://wavemaker.com#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@expo/vector-icons": "~13.0.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/datetimepicker": "^6.7.1",
    "@react-native-community/slider": "^4.2.1",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/drawer": "^6.5.3",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/stack": "^6.3.7",
    "@types/lodash-es": "^4.17.6",
    "axios": "^0.21.1",
    "color": "4.2.3",
    "cross-env": "^5.2.0",
    "css-mediaquery": "^0.1.2",
    "expo-modules-core": "~1.1.0",
    "fast-xml-parser": "^3.17.6",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "react-native-animatable": "^1.3.3",
    "react-native-circular-progress": "^1.3.7",
    "react-native-logs": "^4.0.1",
    "react-native-paper": "^4.11.1",
    "react-native-paper-dates": "^0.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-screens": "~3.18.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.10",
    "react-native-webview": "~11.25.0",
    "react-xml-parser": "^1.1.8",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/color": "^3.0.3",
    "@types/css-mediaquery": "^0.1.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-native": "0.70.5",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "commitlint": "^11.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "execa": "^5.0.0",
    "expo-av": "~13.0.1",
    "expo-barcode-scanner": "~12.1.0",
    "expo-calendar": "~11.0.1",
    "expo-camera": "~13.1.0",
    "expo-contacts": "~11.0.1",
    "expo-device": "~5.0.0",
    "expo-document-picker": "~11.0.1",
    "expo-location": "~15.0.1",
    "expo-network": "~5.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-web-browser": "~12.0.0",
    "handlebars": "^4.7.7",
    "jest": "^29.3.1",
    "jest-html-reporter": "^3.7.0",
    "ncp": "^2.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-builder-bob": "^0.18.1",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-safe-area-context": "4.2.4",
    "react-test-renderer": "18.1.0",
    "release-it": "^14.2.2",
    "tar": "^6.1.0",
    "typescript": "^4.9.3",
    "victory-native": "^36.5.3",
    "yargs": "^16.2.0"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "(test/.*|(\\.|/)(test|spec))\\.[t]sx?$",
    "setupFiles": [
      "./test/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "coverageDirectory": "./dist/reports/coverage",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^lodash-es$": "lodash",
      "^@wavemaker/app-rn-runtime/(.*)$": "<rootDir>/src/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)",
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "reporters": [
      "default",
      [
        "<rootDir>/node_modules/jest-html-reporter",
        {
          "pageTitle": "@wavemaker/app-rn-runtime Test Report",
          "outputPath": "<rootDir>/dist/reports/all-tests.html"
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "ignore": "src/vendor/external.min.js",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": true
    }
  }
}
